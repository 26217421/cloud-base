<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wbw.cloud.dao.UserCredentialsDao">

    <resultMap type="com.wbw.cloud.entity.UserCredentials" id="UserCredentialsMap">
        <result property="username" column="username" jdbcType="VARCHAR"/>
        <result property="type" column="type" jdbcType="VARCHAR"/>
        <result property="userid" column="userId" jdbcType="INTEGER"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="UserCredentialsMap">
        select
          username, type, userId
        from user_credentials
        where username = #{username}
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="username" useGeneratedKeys="true">
        insert into user_credentials(type, userId)
        values (#{type}, #{userid})
    </insert>
</mapper>

